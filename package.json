{
  "name": "code-blocks",
  "version": "1.3.2",
  "repository": "https://github.com/alexwforsythe/code-blocks",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^14.4.0",
    "clean-css-cli": "^4.1.6",
    "google-apps-script": "^2.1.0",
    "highlight.js": "^9.12.0",
    "jquery": "^3.2.1",
    "juice": "^4.1.0",
    "uglify-es": "^3.0.27",
    "uglifyify": "^4.0.3"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dist:gas": ". ./tools.sh && dist_gas",
    "dist:js": ". ./tools.sh && dist_js",
    "dist:static": ". ./tools.sh && dist_static",
    "dist": "npm run dist:gas && npm run dist:js && npm run dist:static",
    "link": "cd ./dist && gas link \"Code Blocks\"",
    "pushOnly": "cd ./dist && gas push",
    "push:js": "npm run dist:js && npm run pushOnly",
    "push:gas": "npm run dist:gas && npm run pushOnly",
    "push": "npm run clean && npm run dist && npm run link && npm run pushOnly"
  }
}
