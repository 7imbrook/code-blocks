{
  "name": "code-blocks",
  "repository": "https://github.com/alexwforsythe/code-blocks",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^14.4.0",
    "google-apps-script": "^2.1.0",
    "highlight.js": "^9.12.0",
    "html-minifier": "^3.5.3",
    "jquery": "^3.2.1",
    "juice": "^4.1.0",
    "uglify-es": "^3.0.27",
    "uglifyify": "^4.0.3"
  },
  "scripts": {
    "clean": "rm -rf ./.gen",
    "prebuild": "npm run clean",
    "build": "browserify -t 'uglifyify' ./client/sidebar.js | uglifyjs > ./client/bundle.min.js",
    "postbuild": "./postbuild.sh",
    "link": "cd ./.gen && gas link $(cat ../.gas/ID)",
    "prepush": "npm run build && npm run link",
    "push": "cd ./.gen && gas push"
  },
  "version": "1.2.0"
}
